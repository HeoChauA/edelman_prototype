<!DOCTYPE html>
<html>
  <head>
    <meta charset="utf-8">
    {% block head %}
      <title>{{ site_title }} - {% block title %}{% endblock %}</title>
      <meta name="viewport" content="width=device-width, initial-scale=1">
      {% include '../blocks/favicons.twig' %}
      <link rel="stylesheet" href="css/style.css">
    {% endblock %}
  </head>
  <body>
    <div class="page-wrapper">
      {% block header %}
        {% include 'header.twig' %}
      {% endblock %}

      {% block feature %}{% endblock %}

      <section role="main" class="content-wrapper">
        <div class="wrapper ">
        {% block content %}
            <h1>{% block title %}{% endblock %}</h1>
        {% endblock %}
        </div>
      </section>
    </div>

    {% block footer %}
      {% include 'footer.twig' %}
    {% endblock %}

    {% include "../blocks/index-links.twig" %}

    {% block footer_scripts %}
      <script src="js/lib/media.match.min.js"></script>
      <script src="js/lib/enquire.min.js"></script>
      <script src="js/lib/jquery.min.js"></script>
      <script type='text/javascript' src='js/alertify.js'></script>
      <script type='text/javascript' src='js/wrap-scripts.js'></script>
      <script type='text/javascript' src='js/skip-link-focus-fix.js'></script>
      <script type='text/javascript' src='js/dropdown.js'></script>
    {% endblock %}
  </body>
</html>
